<tr>
  <td><%= req.start_date %></td>
  <td><%= req.end_date %></td>
  <td>
    <% if req.pending? && current_user.id == @cat.user_id %>
     <tr><%= button_to 'Aprrove', approve_cat_rental_request_url(req) %></tr>
     <tr><%= button_to 'Deny', deny_cat_rental_request_url(req) %></tr>
     <%= req.user.username %>

    <% else %>
      <%= req.status %>
      <%= req.user.username %>
    <% end %>
  </td>
</tr>
